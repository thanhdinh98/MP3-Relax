{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": "airbnb-base",
    "rules": {
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "import/prefer-default-export": 0,
        "no-restricted-syntax": 0,
        "no-param-reassign": 0
    },
    "parser": "babel-eslint"
}